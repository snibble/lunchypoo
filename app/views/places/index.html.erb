<%= form_for :location do |f| %>
  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.submit %>
<% end %>

<h1>Listing places</h1>

<table>
  <tr>
    <th>Name</th>
	<th>Lat</th>
    <th>Long</th>
	<th>Type(s)</th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= place.lat %></td>
    <td><%= place.lng %></td>
  </tr>
<% end %>
</table>

<br />

<br />

<%= link_to 'New Place', new_place_path %>
